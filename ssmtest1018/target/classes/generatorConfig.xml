    <context id="a1" targetRuntime="Mybatis3">

        <!--去除注释 -->
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--数据库连接 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/project"
                        userId="root"
                        password="root">
        </jdbcConnection>

        <!--生成实体类 指定包名 以及生成的地址 （可以自定义地址，但是路径不存在不会自动创建
        使用Maven生成在target目录下，会自动创建） -->
        <javaModelGenerator targetPackage="com.bean"
                            targetProject="C:\ssm1016\ssmtest1018\src\main\java">
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>


        <!--生成SQLmapper文件 -->
        <sqlMapGenerator targetPackage="mapper"
                         targetProject="C:\ssm1016\ssmtest1018\src\main\resources">
        </sqlMapGenerator>
        <!--生成Dao文件,生成接口 -->
